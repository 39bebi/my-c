#include<stdio.h>
int main(){
    int age,usn;
    float weight,height;
    char name,collegename;
    printf("Enter the name,age,usn,weight,height,collegename\n");
    scanf("%c",&name);
    scanf("%d",&age);
    scanf("%d",&usn);
    scanf("%f",&weight);
    scanf("%f",&height);
    scanf("%c",&collegename);
}
